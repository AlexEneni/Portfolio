@model List<Portfolio.Models.ContactSMModels>

@if (Model.Count() != 0)
{
    foreach (var item in Model)
    {
        <dt>
            @item.Name
        </dt>
        if (item.Name != "Phone")
        {
            <dd>
                <a href="@item.value" target="_blank">@item.value</a>
            </dd>
        }
        else
        {
            <dd>
                <span>@item.value</span>
            </dd>
        }
    }
}